{
	"name": "svelte-konva",
	"version": "0.2.0",
	"author": {
		"name": "TeyKey1",
		"url": "https://github.com/TeyKey1"
	},
	"description": "A Svelte wrapper for Konva",
	"homepage": "https://konvajs.org/docs/svelte",
	"bugs": {
		"url": "https://github.com/konvajs/svelte-konva/issues"
	},
	"repository": "github:konvajs/svelte-konva",
	"keywords": [
		"konva",
		"sveltejs",
		"svelte",
		"sveltekit",
		"graphic",
		"html5",
		"canvas"
	],
	"license": "MIT",
	"scripts": {
		"examples": "set NODE_ENV=development&& vite dev",
		"build-app": "vite build",
		"preview": "vite preview -m staging",
		"build": "set NODE_ENV=production&& svelte-kit sync && svelte-package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"plop": "plop",
		"prepack": "plop svelteKonvaComponents&&npm run build",
		"test": "jest src",
		"test:watch": "npm run test -- --watch",
		"pretest": "plop svelteKonvaComponentTests&&plop svelteKonvaComponents",
		"coverage": "jest src --coverage",
		"precoverage": "plop svelteKonvaComponentTests"
	},
	"peerDependencies": {
		"svelte": ">=3",
		"konva": ">=8"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^2.0.1",
		"@sveltejs/kit": "^1.12.0",
		"@sveltejs/package": "^2.0.2",
		"@tailwindcss/typography": "^0.5.7",
		"@testing-library/svelte": "3.1.3",
		"@types/jest": "^29.1.2",
		"@types/lodash.clonedeep": "^4.5.7",
		"@typescript-eslint/eslint-plugin": "^5.44.0",
		"@typescript-eslint/parser": "^5.27.0",
		"daisyui": "^2.31.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"jest": "^29.1.2",
		"jest-environment-jsdom": "^29.1.2",
		"konva": "^9.0.2",
		"lodash.clonedeep": "^4.5.0",
		"plop": "^3.1.1",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"svelte": "^3.44.0",
		"svelte-check": "^3.1.4",
		"svelte-highlight": "^7.2.0",
		"svelte-jester": "^2.3.2",
		"svelte-local-storage-store": "^0.4.0",
		"svelte-preprocess": "^5.0.3",
		"tailwindcss": "^3.1.8",
		"ts-jest": "^29.0.3",
		"tslib": "^2.3.1",
		"typescript": "^5.0.4",
		"vite": "^4.0.0"
	},
	"type": "module",
	"jest": {
		"transform": {
			"^.+\\.svelte$": [
				"svelte-jester",
				{
					"preprocess": "./svelte.config.js"
				}
			],
			"^.+\\.ts$": "ts-jest"
		},
		"moduleFileExtensions": [
			"js",
			"ts",
			"svelte"
		],
		"moduleNameMapper": {
			"^\\$lib(.*)$": "<rootDir>/src/lib$1"
		},
		"testEnvironment": "jsdom",
		"testEnvironmentOptions": {
			"resources": "usable"
		}
	},
