	"description": "A Svelte wrapper for Konva",
	"homepage": "https://konvajs.org/docs/svelte",
	"bugs": {
		"url": "https://github.com/konvajs/svelte-konva/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/konvajs/svelte-konva.git"
	},
	"keywords": [
		"konva",
		"sveltejs",
		"svelte",
		"sveltekit",
		"graphic",
		"html5",
		"canvas"
