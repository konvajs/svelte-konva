	"license": "MIT",
	"scripts": {
		"examples": "set NODE_ENV=development&& vite dev",
		"build-app": "vite build",
		"preview": "vite preview -m staging",
		"build": "set NODE_ENV=production&& svelte-kit sync && svelte-package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"plop": "plop",
		"prepack": "plop svelteKonvaComponents&&npm run build",
		"test": "vitest run src",
		"test:watch": "npm run test -- --watch",
		"pretest": "plop svelteKonvaComponentTests&&plop svelteKonvaComponents",
		"coverage": "vitest run src --coverage",
		"precoverage": "plop svelteKonvaComponentTests"
	},
	"peerDependencies": {
		"konva": "8 - 9",
		"svelte": "^5.0.0"
	},
